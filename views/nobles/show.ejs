<header>
  <h1>Graffiti Nobility</h1>
</header>
<main>
  <div class="container">
    <div class="row">
      <div class="col-lg-9">
        <figure class="image">
          <img src="<%= nobility.image %>" alt="<%= nobility.caption %>" style="width: 65%">
        </figure>
        <div class="column">
          <h4 class="title"><%= nobility.name %></h4>
          <p class="subtitle"><%= nobility.location %></p>
          <p><%= nobility.caption %></p>
        </div>
      </div>
    </div>
  </div>
</main>
<aside class="x">
  <form>
    <div>
      <label>Comment</label>
      <textarea name="text"></textarea>
      <button class="btn btn-secondary">...</button>
    </div>
  </form>
</aside>
