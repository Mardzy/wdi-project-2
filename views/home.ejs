<div class="container hero">
  <div class="overlay">
  </div>
  <div class="row">
  <header id="top" class="main-header col-lg-12">
    <h1 class="col-12">Heaven Spot Heroes</h1>
  </header>
</div>
<div class="row">
  <i class="fa fa-arrow-circle-down arrow col-lg-12" aria-hidden="true"><span class="icon">  TEMPTING YOU...?</span></i>

</div>

</div>
<div class="container-fluid homepage">
  <a class="top" href="#top"><i class="fa fa-arrow-circle-up col-lg-12" aria-hidden="true"><span class="top">  TO TOP</span></i></a>
  <h2 class="types">Street Art Types</h2>
  <article class="row">
    <% types.forEach((type) => { %>
      <div class="photo-image col-lg-6">
          <img class="shadow" src="<%= type.image %>" alt="<%= type.type %>">
          <h5 class="title"><%= type.type %></h5>
      </div>
    <% }); %>
  </article>
  </div>

  <div class="container-fluid terms">
    <article class="personal">
      <h2 class="terms-heading">Community</h2>
        <p>Hi everyone!<br>
        Feel free to register to the site, and maybe even post some photos. I love street art and would enjoy to see it through other points of view. Have a look around, and please try to give credit to the artists if you post</p>
    </article>
  <h2 class="terms-heading">Street Art Terms</h2>
  <article class="row">
    <% terms.forEach((term) => { %>
      <div class="col-lg-12">
        <h5><%= term.headline %></h5>
        <p><%= term.description %></p>
      </div>
    <% }); %>
  </article>
</div>
